@font-face {
  font-family: 'Fakt Pro';
  src: local('Fakt Pro Blond'),
       url('https://cdn.auth0.com/styleguide/core/3.0.0/fonts/fakt/faktpro-blond.woff2') format('woff2'),
       url('https://cdn.auth0.com/styleguide/core/3.0.0/fonts/fakt/faktpro-blond.woff') format('woff'),
       url('https://cdn.auth0.com/styleguide/core/3.0.0/fonts/fakt/faktpro-blond.ttf') format('truetype');
  font-weight: 300;
  font-style: normal;
}

